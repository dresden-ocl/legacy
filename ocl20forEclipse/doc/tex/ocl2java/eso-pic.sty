\NeedsTeXFormat{LaTeX2e}[1995/01/25]
\ProvidesPackage{eso-pic}
   [1998/07/11 v0.5 eso-pic -- extension to `everyshi' (RN)]

\RequirePackage{everyshi}[1995/01/25]

\newcommand{\@ShipoutPicture@Hook}{}
\newcommand{\AddToShipoutPicture}{\g@addto@macro\@ShipoutPicture@Hook}
\newcommand{\ClearShipoutPicture}{\let\@ShipoutPicture@Hook\@empty}
\newcommand{\@ShipoutPicture}{%
  \ifx\@ShipoutPicture@Hook\@empty
  \else
    \bgroup
      \@tempdima=1in
      \@tempcnta=\@tempdima
      \@tempcntb=-\@tempdima
      \advance\@tempcntb\paperheight
      \global\setbox\@cclv\vbox{%
      \vbox{%
        \unitlength=1sp%
        \begin{picture}(0,0)(\@tempcnta,\@tempcntb)%
          \let\protect\relax\@ShipoutPicture@Hook%
        \end{picture}}%
        \box\@cclv%
      }%
    \egroup
  \fi
}

\EveryShipout{\@ShipoutPicture}

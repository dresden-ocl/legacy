package uml 


-- Library: OCLVoids

--	A library with examples for OclVoid.
-- 
--	Author: Joanna Chimiak-Opoka
--	Author: Hannes Mösl
--------------------------------------------------------
-- tests (queries with boolean result)
--------------------------------------------------------
--	Testing OclUdefined type and its only instantiation, i.
-- e. null.
context Model

inv tOCLAnyUndefined_1: let v:OclVoid = null in v.oclIsUndefined()

context Model

inv tOCLAnyUndefined_2: let v:OclVoid = null in not v.oclIsInvalid()

context Model

inv tOCLAnyUndefined_3: let v:OclVoid = null in v = v


-- OclSpec:
-- However, by virtue of the implicit conversion to a collection
-- literal, an expression evaluating to null can be used as 
-- source of collection operations (such as ‘isEmpty’). If the source is
-- the null literal, it is implicitly converted to Bag{}.


-- parse-error
context Model
inv tOclAnyUndefined_3a: let v:OclVoid = null in v.isEmpty()
  
context Model 
inv tOCLAnyUndefined_3b: let v:OclVoid = null in not v.oclIsInvalid() and v.oclIsUndefined()



context Model

inv tOCLAnyUndefined_4: let v:OclVoid = null in not (v <> 1)

context Model

inv tOCLAnyUndefined_5: let v:OclVoid = null in Set{null}->size() = 0

endpackage
-- B5: forAll2collect_one
-------------------------------------


-- Collection Operations

-- -- Collect

-- -- -- forAll to collect and one

-- -- -- -- Empty

-- -- -- -- -- Set ------------------------------------------------

context Dummy inv: 
	let c:Set(Integer) = Set{} in 
   c->forAll(i|i<4) = 
         (let s = c->collect(i|i<4)->asSet() in 
       c->notEmpty() implies s->one(true) and s->one(e|e))

  

package tudresden::ocl20::benchmark::testdata::common

-------------------------------------
-- B5: exists2select
-------------------------------------


-- Collection Operations

-- -- Interdisciplinary

-- -- -- exists to select

-- -- -- -- Empty

-- -- -- -- -- Set ------------------------------------------------

context Dummy inv: 
	let c:Set(Integer) = Set{} in
   c->exists(i|i<4) = c->select(i|i<4)->notEmpty()

  -- Expected Result:
  = true

-- -- -- -- -- Bag

context Dummy inv: 
	let c:Bag(Integer) = Bag{} in
   c->exists(i|i<4) = c->select(i|i<4)->notEmpty()

  -- Expected Result:
  = true

-- -- -- -- -- Sequence


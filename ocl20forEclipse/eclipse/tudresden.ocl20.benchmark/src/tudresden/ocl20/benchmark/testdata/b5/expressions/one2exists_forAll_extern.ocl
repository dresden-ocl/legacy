-- Collection Operations

-- -- Logic

-- -- -- one to exists and forAll (extern)

-- -- -- -- Empty

-- -- -- -- -- Set ------------------------------------------------

context Dummy inv: 
	let c:Set(Integer) = Set{} in
   c->one(i|i<4) =
         (c->exists(i|i<4) and c->forAll(x,y|x<4 and y<4 implies x = y))

  -- Expected Result:
  = true


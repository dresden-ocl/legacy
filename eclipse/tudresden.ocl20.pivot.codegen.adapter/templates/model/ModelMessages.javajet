<%@ jet package="tudresden.ocl20.pivot.codegen.adapter.templates.model" class="ModelMessages" imports="org.eclipse.emf.codegen.ecore.genmodel.* tudresden.ocl20.pivot.codegen.adapter.PivotAdapterGeneratorUtil "%>

<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="tudresden.ocl20.eclipseplugin"
   name="OCLToolkitForEclipse Plug-in"
   version="2.0.0"
   provider-name=""
   class="tudresden.ocl20.eclipse.OCLToolkitForEclipsePlugin">

   <runtime>
      <library name="OCLToolkitForEclipse.jar">
         <export name="*"/>
      </library>
      <library name="lib/external/antlr.jar">
         <export name="*"/>
      </library>
      <library name="lib/external/stringtemplate.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
   </requires>
</plugin>

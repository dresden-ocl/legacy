// --- Max Operation ---
maxOperation(sourceExp, argsExp) ::= <<
java.lang.Math.max($sourceExp$, $argsExp$)
>>


// --- Max Operation on Collection ---
maxOnCollectionOperation(sourceExp, genericType, resultVar, elementName) ::= <<
$genericType$ $resultVar$;
$resultVar$ = $undefinedLiteralExp()$;

/* Compute the result of a max operation. */
for ($genericType$ $elementName$ : $sourceExp$) {
    if ($resultVar$ == $undefinedLiteralExp()$ || ((Comparable) $resultVar$).compareTo((Comparable) $elementName$) < 0) {
        $resultVar$ = $elementName$;
    }
}

>>


// --- Min Operation ---
minOperation(sourceExp, argsExp) ::= <<
java.lang.Math.min($sourceExp$, $argsExp$)
>>


// --- Min Operation on Collection ---
minOnCollectionOperation(sourceExp, genericType, resultVar, elementName) ::= <<
$genericType$ $resultVar$;
$resultVar$ = $undefinedLiteralExp()$;

/* Compute the result of a min operation. */
for ($genericType$ $elementName$ : $sourceExp$) {
    if ($resultVar$ == $undefinedLiteralExp()$ || ((Comparable) $resultVar$).compareTo((Comparable) $elementName$) > 0) {
        $resultVar$ = $elementName$;
    }
}

>>



$sourceExp$.count($argsExp$)
>>


// --- Div Operation ---
divOperation(sourceExp, argsExp) ::= <<
($sourceExp$ / $argsExp$)
>>


// --- Division Operation ---
divisionOperation(sourceExp, argsExp) ::= <<
($sourceExp$ / $argsExp$)
>>


// --- Division Operation On Integer ---
divisionOnIntegerOperation(sourceExp, argsExp) ::= <<

package tudresden::ocl20::benchmark::testdata::common

-------------------------------------
-- B5: reject2iterate
-------------------------------------

-- Collection Operations

-- -- Iterate

-- -- -- reject

-- -- -- -- Empty

-- -- -- -- -- Set ------------------------------------------------

context Dummy inv: 
	let c:Set(Integer) = Set{} in 
   c->reject(i|i<4) = 
     c->iterate(i; r:Set(Integer) = Set{} | 
           if i<4 then r else r->including(i) endif)

  -- Expected Result:
  = true


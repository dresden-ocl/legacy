package tudresden::ocl20::benchmark::testdata::common

-------------------------------------
-- B5: forall2exists
-------------------------------------


-- Collection Operations

-- -- Logic

-- -- -- forAll to exists

-- -- -- -- Empty

-- -- -- -- -- Set ------------------------------------------------

context Dummy inv:
	let c:oclEmpty(Integer) = Set{} in
   c->forAll(i|i<4) = not c->exists(i|not(i<4))

Expected Results:
  true : Boolean

-- -- -- -- -- Bag

context Dummy inv:
	let c:oclEmpty(Integer) = Bag{} in
   c->forAll(i|i<4) = not c->exists(i|not(i<4))

Expected Results:
  true : Boolean


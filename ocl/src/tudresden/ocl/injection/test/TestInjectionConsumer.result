package// hallo
  tudresden.ocl.injection.test[onPackage];

import java.util.*[onImport];
import java.io.BufferedReader[onImport];
import java.text.Format[onImport];

/**
   Represents an attribute or association partner of a class.
   Note: type==Model.AMIGOUS means, the attribute cannot be used in OCL due to attribute ambiguities.
   See OCL spec 5.4.1. for details.
*/
public abstract class Example[onClass] implements Runnable
{
  [onAttributeHeader]private String name;[onClassFeature]
  [onAttributeHeader]private Integer type=new Integer(5);[onClassFeature]
  [onAttributeHeader]private volatile Integer[] qualifiers;[onClassFeature]
  [onAttributeHeader]String hallo="hallo";[onClassFeature]
  
  [onAttributeHeader]int commaSeparated1,commaSeparated2=0,commaSeparated3;[onClassFeature][onClassFeature][onClassFeature] 
  [onAttributeHeader]int commaSeparated4=80,commaSeparated5,commaSeparated6=200;[onClassFeature][onClassFeature][onClassFeature] 

  // these attributes test the ability of the parser
  // to skip more complex (ugly) attribute initializers
  [onAttributeHeader]String   uglyAttribute1="some'Thing{some\"Thing;Else";[onClassFeature]
  [onAttributeHeader]char     uglyAttribute2=';';[onClassFeature]
  [onAttributeHeader]char     uglyAttribute3='{';[onClassFeature]
  [onAttributeHeader]char     uglyAttribute4='"';[onClassFeature]
  [onAttributeHeader]char     uglyAttribute5='\'';[onClassFeature]
  [onAttributeHeader]String[] uglyAttribute6=
  {
    "some'Thing{some\"Thing;Else", // ugly ; { " ' comment
    "some'Thing{some\"Thing;Else"
  };[onClassFeature]
  [onAttributeHeader]char[]   uglyAttribute7={';','{','"','\''};[onClassFeature]
  [onAttributeHeader]Runnable uglyAttribute8=new Runnable()
  {
    // ugly ; { " ' comment
    String   uglyInnerAttribute1="some'Thing{some\"Thing;Else";
    char     uglyInnerAttribute2=';';
    char     uglyInnerAttribute3='{';
    char     uglyInnerAttribute4='"';
    char     uglyInnerAttribute5='\'';
    String[] uglyInnerAttribute6=
    {
      "some'Thing{some\"Thing;Else", // ugly ; { " ' comment
      "some'Thing{some\"Thing;Else"
    };
    char[]   uglyInnerAttribute7={';','{','"','\''};
    public void run()
    {
      // ugly ; { " ' comment
      String   uglyVariable1="some'Thing{some\"Thing;Else";
      char     uglyVariable2=';';
      char     uglyVariable3='{';
      char     uglyVariable4='"';
      char     uglyVariable5='\'';
      String[] uglyVariable6=
      {
        "some'Thing{some\"Thing;Else", // ugly ; { " ' comment
        "some'Thing{some\"Thing;Else"
      };
      char[]   uglyAttribute7={';','{','"','\''};
    }
    // ugly ; { " ' comment
  };[onClassFeature]
  // end of ugly attributes
  

  [onClass]class Inner implements Runnable
  {
    [onClass]class Drinner implements Runnable
    {
      [onAttributeHeader]boolean someDrinnerBoolean=true;[onClassFeature]
    
      [onBehaviourHeader]public void run()
      {
      }[onClassFeature]
    [onClassEnd][onClassFeature]}

    [onAttributeHeader]boolean someInnerBoolean=true;[onClassFeature]
    
    [onBehaviourHeader]public void run()
    {
    }[onClassFeature]
  [onClassEnd][onClassFeature]}  

  [onDocComment]/**
    * Testcomment
    */
  [onAttributeHeader]private int a;[onClassFeature]
  
  [onAttributeHeader]private List collectionWithoutComment;[onClassFeature]
  
  [onDocComment]/**
    * Testcomment
    */
  [onAttributeHeader]private int b;[onClassFeature]
  
  [onAttributeHeader]private Map mapWithoutComment;[onClassFeature]

  [onBehaviourHeader]private Example()
  {
    namedIntegers.put("5", new Integer(5));
  }[onClassFeature]
  
  [onBehaviourHeader]public Example(String name, Integer type)
  {
    super();
    qualifiers=new Integer[6];
    namedIntegers.put("5", new Integer(5));
  }[onClassFeature]

  [onBehaviourHeader]public void set(String name, Integer type,// what a cool parameter
    final Integer[] qualifiers)
  {
    if(name==null)
      throw new IllegalArgumentException(); // ugly comment : { {
    this.name=name;
    String x="ugly { string \" { literal";
    char c='{';

    /**
      ugly comment *
    **/
    if(type==null)
      throw new IllegalArgumentException(); // some other comment
    this.type=type;

    if(qualifiers!=null&&qualifiers.length==0)
      throw new IllegalArgumentException();
    this.qualifiers=qualifiers;
    
    int a=20;
    a=a/(a+b); // ugly expression
  }[onClassFeature]

  [onBehaviourHeader]abstract void abstractMethod();[onClassFeature]

  [onDocComment]/**
     Some example doc-comment.
  */
  [onBehaviourHeader]public void run()
  {}[onClassFeature]

  [onDocComment]/**
     A collection of Strings.
     @element-type java.lang.String
     @see java.lang.String
     @invariant stringsMinusGreaterincludes_inlinehallo_:        strings->includes('inlinehallo')
     @invariant stringsMinusGreaterincludes_inline_space_hallo_: strings->includes('inline space hallo')
  */
  [onAttributeHeader]Set myStrings=new HashSet();[onClassFeature]
  
  [onDocComment]/**
     @element-type Integer
  */
  [onAttributeHeader]Set integers=new HashSet();[onClassFeature]
  
  [onAttributeHeader]Integer anInteger=new Integer(5);[onClassFeature]
  
  [onDocComment]/**
    * A collection of dates.
    *
    * Here are some lines to test the reveng GUI....
    *
     @element-type Date
  */
  [onAttributeHeader]List dates=new ArrayList();[onClassFeature]
  
  [onAttributeHeader]Date aDate=new Date();[onClassFeature]
  
  [onDocComment]/**
     @element-type AbstractImplementation
  */
  [onAttributeHeader]Set interfaces=new HashSet();[onClassFeature]
  
  [onAttributeHeader]AbstractImplementation anInterface=new Implementation();[onClassFeature]
  
  [onDocComment]/**
     @element-type Format
  */
  [onAttributeHeader]Vector formats=new Vector();[onClassFeature]
  
  [onAttributeHeader]Format aFormat=new java.text.DecimalFormat();[onClassFeature]

  [onDocComment]/**
  *
  * A map from strings to integers.
  * * * * *  These stars should be removed by the reveng GUI...
  
  *
     @element-type Integer
     @key-type String
  */
  [onAttributeHeader]HashMap namedIntegers=new HashMap();[onClassFeature]
  
  [onBehaviourHeader]public boolean poly1(Interface someInterface)
  {
    return true;
  }[onClassFeature]

  [onBehaviourHeader]public String getName()
  {
    return name;
  }[onClassFeature]

  [onBehaviourHeader]public Integer getType()
  {
    return type;
  }[onClassFeature]

  [onDocComment]/**
     @precondition  stringsMinusGreaterincludes_inlinehallopre_:          strings->includes('inlinehallopre')
     @precondition  stringsMinusGreaterincludes_inline_space_hallo_pre_:  strings->includes('inline space hallo pre')
     @postcondition stringsMinusGreaterincludes_inlinehallopost_:         strings->includes('inlinehallopost')
     @postcondition stringsMinusGreaterincludes_inline_space_hallo_post_: strings->includes('inline space hallo post')
  */
  [onBehaviourHeader]public Integer[] getQualifiers()
  {
    namedIntegers.put("10", new Integer(10));
    return qualifiers;
  }[onClassFeature]

  [onAttributeHeader]public Integer unqualifiedType=null;[onClassFeature]

  [onBehaviourHeader]public Integer getUnqualifiedType() throws IllegalArgumentException
  {
    if(unqualifiedType!=null)
      return unqualifiedType;

    if(qualifiers==null)
      throw new IllegalArgumentException();

    unqualifiedType=
      (type instanceof Integer) ? type : type;
    return unqualifiedType;
  }[onClassFeature]

  [onAttributeHeader]private Object parent;[onClassFeature]

  [onBehaviourHeader]public void setParent  (Object parent)
    throws
      IllegalArgumentException,
      NullPointerException
  {
    if(this.parent==null)
      this.parent=parent;
    else
      throw new IllegalArgumentException("An attributes parent cannot be set twice.");
  }[onClassFeature]

  [onBehaviourHeader]public Object getParent()
  {
    return parent;
  }[onClassFeature]

  [onBehaviourHeader]public void printData
    (java.io.PrintStream o)
  {
  }[onClassFeature]
  
  [onBehaviourHeader]private   void accessifierPrivate() {}[onClassFeature]
  [onBehaviourHeader]protected void accessifierProtected() {}[onClassFeature]
            [onBehaviourHeader]void accessifierPackage() {}[onClassFeature]
  [onBehaviourHeader]public    void accessifierPublic() {}[onClassFeature]
  
  [onBehaviourHeader]void test_super()
  {
  }[onClassFeature]

  [onBehaviourHeader]static public void main(String[] args)
  {
    tudresden.ocl.lib.Ocl.TOLERATE_NONEXISTENT_FIELDS=false;
    tudresden.ocl.lib.Ocl.setNameAdapter(new tudresden.ocl.lib.ArgoNameAdapter());
    SecondExample e2=new SecondExample();
    e2.getQualifiers();
    e2.i=10;
    e2.anInteger=new Integer(8);
    e2.getQualifiers();
    e2.test_super();
  }[onClassFeature]

[onClassEnd]}

class SecondExample[onClass] extends Example{
  [onAttributeHeader]int i;[onClassFeature]
  
  [onDocComment]/**
     @invariant testingInheritance: self.hallo='prollo'
  */
  [onBehaviourHeader]SecondExample()
  {
    super("somename", new Integer(5));
  }[onClassFeature]
  
  {
    // Object initializer as defined in Java Language Spec D.1.3
    i=10;
  }

  [onBehaviourHeader]void abstractMethod()  {}[onClassFeature]
  
  [onDocComment]/**
     Tests, whether injection does not produce infinite loops,
     when wrapping methods with calls to super.
  */
  [onBehaviourHeader]void test_super()
  {
    super.test_super();
  }[onClassFeature]
  
  static
  {
    // this has to be tested too.
  }
  
[onClassEnd]}

class ThirdExample[onClass] extends SecondExample
{
  // this class has no explicit constructor
  [onBehaviourHeader]void abstractMethod()  {}[onClassFeature]
[onClassEnd]}

interface ExampleFour[onClass]
{
  [onBehaviourHeader]public int someMethod(double x);[onClassFeature]
[onClassEnd]}
